 
<table class="table table-hover" id = "table-<%= tab_index %>" >
  <thead>
    <tr>
      <th>Index</th>
      <th>欄</th>
      <th>Range</th>
      <th>Tab Id</th>
      <th>資料類型</th>
      <th>XML tag</th>
      <th></th>
    </tr>    
  </thead>
  <tbody>
    <% @columns.each do |column| %>
      <tr class="col-<%= column[:index] %>">
        <%= render(:partial => "tags/row", :locals => {:column => column}) %>
      </tr>
    <% end %>
  </tbody>
   
 
 </table>